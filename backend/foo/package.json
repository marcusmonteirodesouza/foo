    "dev": "concurrently \"firebase emulators:start --only firestore,auth\" \"ts-node-dev -r dotenv/config src/index.ts\"",
    "prestart": "npm run compile",
    "start": "node -r dotenv/config build/src/index.js",
    "predeploy": "npm run compile",
    "deploy": "node build/scripts/deploy"
